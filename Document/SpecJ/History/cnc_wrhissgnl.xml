        </lc>
        <!-- P（パンチプレス） -->
        <p>
            <fs0id>X</fs0id>
            <fs0if>X</fs0if>
            <fs16i>O</fs16i>
            <fs18i>O</fs18i>
            <fs30ia>X</fs30ia>
