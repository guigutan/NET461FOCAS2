            <fs21ia>X</fs21ia>
            <fs16ib>O</fs16ib>
            <fs18ib>O</fs18ib>
            <fs21ib>O</fs21ib>
            <fs30ia>X</fs30ia>
            <fs30ib>X</fs30ib>
        </m>
        <!-- T（旋盤） -->
        <t>
