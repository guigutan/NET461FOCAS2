<?xml version="1.0" encoding="Shift_JIS"?>
<?xml-stylesheet type="text/xsl" href="../Fmethod_xsl/fmethod_he_c.xsl"?>
<root>
    <func>


        <!-- 関数名 -->
        <title>cnc_getpath</title>



        <!-- 関数宣言 -->
        <declare>
            <!-- Visual C++の宣言 -->
            <vc>
                <include>#include "fwlib32.h"</include>
                <prottype>FWLIBAPI short WINAPI cnc_getpath(unsigned short FlibHndl, short *path_no, short *maxpath_no);</prottype>
            </vc>
        </declare>



        <!-- 解説 -->
        <doc>
            <cmn>
               <p>
                   関数の処理対象となっている系統を読み出します。
               </p>
            </cmn>
        </doc>



        <!-- 引数 -->
        <argument>
            <common>
                <cmn>
                    <item>
                        <name>FlibHndl</name>
                        <type>in</type>
                        <content>
                            <p>
                                ライブラリハンドルです。詳細は、「<a href="handle.htm" target="_parent">ライブラリハンドル</a>」を参照して下さい。
                            </p>
                        </content>
                    </item>
                    <item>
                        <name>path_no</name>
                        <type>out</type>
                        <content>
                            <p class="dd">
                            系統番号を取得する変数のアドレスを指定します。
                            </p>
                            <table class="dd">
                                <tr>
                                    <td align="right">0</td>
                                    <td>:</td>
                                    <td>(系統 1)</td>
                                </tr>
                                <tr>
                                    <td align="right">1</td>
                                    <td>:</td>
                                    <td>系統 1</td>
                                </tr>
                                <tr>
                                    <td align="right">2</td>
                                    <td>:</td>
                                    <td>系統 2</td>
                                </tr>
                                <tr>
                                    <td align="right">3</td>
                                    <td>:</td>
                                    <td>系統 3</td>
                                </tr>
                                <tr>
                                    <td align="right">4</td>
                                    <td>:</td>
                                    <td>系統 4 (Series 30i のみ)</td>
                                </tr>
                                <tr>
                                    <td align="right">5</td>
                                    <td>:</td>
                                    <td>系統 5/ローダ</td>
                                </tr>
                                <tr>
                                    <td align="right">6</td>
                                    <td>:</td>
                                    <td>系統 6 (Series 30i のみ)</td>
                                </tr>
                                <tr>
                                    <td align="right">7</td>
                                    <td>:</td>
                                    <td>系統 7 (Series 30i のみ)</td>
                                </tr>
                                <tr>
                                    <td align="right">8</td>
                                    <td>:</td>
                                    <td>系統 8 (Series 30i のみ)</td>
                                </tr>
                                <tr>
                                    <td align="right">9</td>
                                    <td>:</td>
                                    <td>系統 9 (Series 30i のみ)</td>
                                </tr>
                                <tr>
                                    <td align="right">10</td>
                                    <td>:</td>
                                    <td>系統 10 (Series 30i のみ)</td>
                                </tr>
                            </table>
                        </content>
                    </item>
                    <item>
                        <name>maxpath_no</name>
                        <type>out</type>
                        <content>
                            <p class="dd">
                                最大系統番号を取得する変数のアドレスを指定します。
                            </p>
                        </content>
                    </item>
                </cmn>
            </common>
            <!-- 30i(WinCE) -->
            <is>
                <hssb>
                    <vc>
                        <item>
                            <name>FlibHndl</name>
                            <type>in</type>
                            <content>
                                <p>
                                ライブラリハンドルです。詳細は、「<a href="handle.htm" target="_parent">ライブラリハンドル</a>」を参照して下さい。
                                </p>
                            </content>
                        </item>
                        <item>
                            <name>path_no</name>
                            <type>out</type>
                            <content>
                                系統番号を取得する変数のアドレスを指定します。
                            </content>
                        </item>
                        <item>
                            <name>maxpath_no</name>
                            <type>out</type>
                            <content>
                                最大系統番号を取得する変数のアドレスを指定します。
                            </content>
                        </item>
                    </vc>
                </hssb>
            </is>
        </argument>



        <!-- 戻り値 -->
        <errcode>
            <add></add>
        </errcode>



        <!-- CNCオプション -->
        <option>
            <hssb>拡張ドライバ/ライブラリ機能が必要です。</hssb>
            <ethr><p>イーサネット機能と拡張ドライバ/ライブラリ機能が必要です。</p>
                  <p>但し、Series 16i/18i/21i-B, 0i-B/C/D/F, Series 30i, PMi-A に関しては次のようになります。</p>
                  <p>組込みイーサネットを使用のとき、</p>
                     <p class="dd">上記２つの機能は必要ありません。</p>
                  <p>イーサネットボードを使用のとき、</p>
                      <p class="dd">イーサネット機能のみ必要です。</p>
            </ethr>
        </option>



        <!-- CNCパラメータ -->
        <parameter></parameter>



        <!-- CNCモード -->
        <mode></mode>



        <!-- 対応機種 -->
        <support>
            <!-- M（ミリング） -->
            <!-- M（ミリング） -->
            <m>
                <fs0ia>H</fs0ia>
                <fs0ib>O</fs0ib>
                <fs0id>O</fs0id>
                <fs0if>O</fs0if>
                <fs15>H</fs15>
                <fs15i>O</fs15i>
                <fs16>H</fs16>
                <fs18>H</fs18>
                <fs21>H</fs21>
                <fs16ia>O</fs16ia>
                <fs18ia>O</fs18ia>
                <fs21ia>O</fs21ia>
                <fs16ib>O</fs16ib>
                <fs18ib>O</fs18ib>
                <fs21ib>O</fs21ib>
                <fs30ia>O</fs30ia>
                <fs30ib>O</fs30ib>
            </m>
            <!-- T（旋盤） -->
            <t>
                <fs0ia>H</fs0ia>
                <fs0ib>O</fs0ib>
                <fs0id>O</fs0id>
                <fs0if>O</fs0if>
                <fs15>H</fs15>
                <fs15i>X</fs15i>
                <fs16>H</fs16>
                <fs18>H</fs18>
                <fs21>X</fs21>
                <fs16ia>O</fs16ia>
                <fs18ia>O</fs18ia>
                <fs21ia>O</fs21ia>
                <fs16ib>O</fs16ib>
                <fs18ib>O</fs18ib>
                <fs21ib>O</fs21ib>
                <fs30ia>O</fs30ia>
                <fs30ib>O</fs30ib>
            </t>
            <!-- LC（ローダ制御） -->
            <lc>
                <fs0ia>X</fs0ia>
                <fs0ib>X</fs0ib>
                <fs0id>X</fs0id>
                <fs0if>X</fs0if>
                <fs15>X</fs15>
                <fs15i>X</fs15i>
                <fs16>H</fs16>
                <fs18>H</fs18>
                <fs21>H</fs21>
                <fs16ia>H</fs16ia>
                <fs18ia>H</fs18ia>
                <fs21ia>H</fs21ia>
                <fs16ib>H</fs16ib>
                <fs18ib>H</fs18ib>
                <fs21ib>H</fs21ib>
                <fs30ia>X</fs30ia>
                <fs30ib>X</fs30ib>
            </lc>
            <!-- P（パンチプレス） -->
            <p>
                <fs0id>O</fs0id>
                <fs0if>O</fs0if>
                <fs16i>O</fs16i>
                <fs18i>O</fs18i>
                <fs30ia>X</fs30ia>
                <fs30ib>O</fs30ib>
            </p>
            <!-- L（レーザ） -->
            <l>
                <fs0id>X</fs0id>
                <fs0if>X</fs0if>
                <fs16i>O</fs16i>
                <fs18i>X</fs18i>
                <fs30ia>X</fs30ia>
                <fs30ib>O</fs30ib>
            </l>
            <!-- W（ワイヤカット） -->
            <w>
                <fs0id>X</fs0id>
                <fs0if>X</fs0if>
                <fs16i>X</fs16i>
                <fs18i>X</fs18i>
                <fs30ia>O</fs30ia>
                <fs30ib>O</fs30ib>
            </w>
            <!-- PM（パワーメート） -->
            <pw>
                <d>X</d>
                <h>X</h>
                <a>O</a>
            </pw>
            <!-- 30i(WinCE) -->
            <is>
                <!-- M（ミリング） -->
                <m>
                    <fs30isa>O</fs30isa>
                    <fs30isb>O</fs30isb>
                </m>
                <!-- T（旋盤） -->
                <t>
                    <fs30isa>O</fs30isa>
                    <fs30isb>O</fs30isb>
                </t>
                <!-- W（ワイヤカット） -->
                <w>
                    <fs30isa>O</fs30isa>
                    <fs30isb>O</fs30isb>
                </w>
            </is>
        </support>



        <!-- 参照 -->
        <reference>
            <item>
                <name>cnc_setpath</name>
                <file>misc/cnc_setpath.htm</file>
            </item>
        </reference>



        <!-- プログラム例 -->
        <example>
<vc><![CDATA[
以下のプログラムは現在の処理対象系統を読みだして表示します。

#include "fwlib32.h"

void example( void )
{
        short path, max ;
        cnc_getpath( h, &path, &max ) ;
        printf( "Current target path is PATH #%d (max:%d).\n", path, max ) ;
}]]></vc>
        </example>



    </func>
</root>
