<root>
    <func>


        <!-- 関数名 -->
        <title>cnc_rdzofs</title>



        <!-- 関数宣言 -->
        <declare>
            <!-- Visual C++の宣言 -->
            <vc>
                <include>#include "fwlib32.h"</include>
                <prottype>
                    FWLIBAPI short WINAPI cnc_rdzofs(unsigned short FlibHndl, short number, short axis, short length, IODBZOFS *zofs); 
                </prottype>
            </vc>
        </declare>



        <!-- 解説 -->
