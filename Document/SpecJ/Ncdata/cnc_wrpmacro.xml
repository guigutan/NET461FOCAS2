            <fs30ia>O</fs30ia>
            <fs30ib>O</fs30ib>
        </w>
        <!-- PM（パワーメート） -->
        <pw>
            <d>O</d>
            <h>O</h>
            <a>O</a>
        </pw>

        <is>
            <m>
                <fs30isa>O</fs30isa>
                <fs30isb>O</fs30isb>
            </m>
            <t>
                <fs30isa>O</fs30isa>
                <fs30isb>O</fs30isb>
            </t>
            <!-- W（ワイヤカット） -->
            <w>
                <fs30isa>O</fs30isa>
