<?xml version="1.0" encoding="Shift_JIS"?>
<?xml-stylesheet type="text/xsl" href="../Fmethod_xsl/fmethod_he_c.xsl"?>

<root>
  <func>



    <!-- 関数名 -->
    <title>cnc_clr5axpls</title>



    <!-- 関数宣言 -->
    <declare>
      <!-- Visual C++の宣言 -->
      <vc>
        <include>#include "fwlib32.h"</include>
        <prottype>
            FWLIBAPI short WINAPI cnc_clr5axpls(unsigned short FlibHndl, short type);
        </prottype>
      </vc>
    </declare>



    <!-- 解説 -->
    <doc>
        <cmn>
            <p>５軸加工手動送りパルス量をクリアします。 </p>
        </cmn>
    </doc>



    <!-- 引数 -->
    <argument>
        <common>
            <vc>
                <item>
                    <name>FlibHndl</name>
                    <type>in</type>
                    <content>
                        <p>
                            ライブラリハンドルです。詳細は、「<a href="handle.htm" target="_parent">ライブラリハンドル</a>」を参照して下さい。
                        </p>
                    </content>
                </item>
                <item>
                    <name>type</name>
                    <type>in</type>
                    <content>
                        <p>
                           クリアするパルス量を指定します。
                        </p>

                        <table border="1" class="dd">
                          <tr><td>type</td><td>クリアするパルス量</td></tr>
                          <tr><td> 1  </td><td>工具先端中心 C1   </td></tr>
                          <tr><td> 2  </td><td>工具先端中心 C2   </td></tr>
                          <tr><td> 3  </td><td>工具軸基準 TD     </td></tr>
                          <tr><td> 4  </td><td>工具軸基準 R1     </td></tr>
                          <tr><td> 5  </td><td>工具軸基準 R2     </td></tr>
                          <tr><td> 6  </td><td>テーブル基準 VR   </td></tr>
                          <tr><td> 7  </td><td>テーブル基準 H1   </td></tr>
                          <tr><td> 8  </td><td>テーブル基準 H2   </td></tr>
                          <tr><td>-1  </td><td>全パルス量        </td></tr>
                        </table>
                    </content>
                </item>
            </vc>
        </common>
    </argument>



    <!-- 戻り値 -->
    <errcode>
        <item>
            <name>EW_NUMBER</name>
            <content>type の指定が正しくありません。</content>
        </item>
        <item>
            <name>EW_NOOPT</name>
            <content>オプションなし<br/>
                     ５軸加工用手動送り機能が必要です。</content>
        </item>
    </errcode>



    <!-- CNCオプション -->
    <option>
        <hssb>拡張ドライバ/ライブラリ機能および、５軸加工用手動送り機能が必要です。</hssb>
    </option>



