            <fs0id>X</fs0id>
            <fs0if>X</fs0if>
            <fs16i>H</fs16i>
            <fs18i>H</fs18i>
            <fs30ia>H</fs30ia>
            <fs30ib>H</fs30ib>
        </w>
        <!-- PM（パワーメート） -->
        <pw>
            <d>H</d>
