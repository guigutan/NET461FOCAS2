    <example>
<vc>
以下のプログラムは指定されたシーケンス番号をサーチし、結果を表示します。

#include "fwlib32.h"

/* no is sequence number to be searched. */
void example( long no )
{
        short ret ;
        ret = cnc_seqsrch2( h, no ) ;
        switch ( ret ) {
                case EW_OK:
                        printf( "N%d has been searched.\n", no ) ;
                        break;
                case EW_DATA:
                        printf( "N%d doesn't exist.\n", no ) ;
                        break;
                case EW_MODE:
                        printf( "ILLIGAL MODE.\n" ) ;
                        break;
                case EW_REJECT:
                        printf( "REJECTED.\n" ) ;
                        break;
        }
}
</vc>
