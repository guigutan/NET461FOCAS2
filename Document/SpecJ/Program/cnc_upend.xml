<?xml version="1.0" encoding="Shift_JIS"?>
<?xml-stylesheet type="text/xsl" href="../Fmethod_xsl/fmethod_he_c.xsl"?>

<root>
  <func>


    <!-- 関数名 -->
    <title>cnc_upend</title>


    <!-- 関数宣言 -->
    <declare>
      <!-- Visual C++の宣言 -->
      <vc>
        <include>#include "fwlib32.h"</include>
        <prottype>FWLIBAPI short WINAPI cnc_upend(unsigned short FlibHndl);</prottype>
      </vc>
    </declare>



    <!-- 解説 -->
    <doc>
        <cmn>
            <p>NCデータのリード(アップロード)の終了を通知します。 <br/>
                (アップロードを行った後、必ず実行する必要があります。)
            </p>
        </cmn>
    </doc>



    <!-- 引数 -->
    <argument>
        <hssb>
            <cmn>
                <item>
                    <name>FlibHndl</name>
                    <type>in</type>
                    <content>
                        <p>
                            ライブラリハンドルです。詳細は、「<a href="handle.htm" target="_parent">ライブラリハンドル</a>」を参照して下さい。
                        </p>
                    </content>
                </item>
            </cmn>
        </hssb>
    </argument>



    <!-- 戻り値 -->
    <errcode>
        <item>
            <name>EW_FUNC</name>
            <content>
                <a href="Program/cnc_upstart.htm" target="_parent">cnc_upstart</a>関数が実行されていません。
            </content>
        </item>
    </errcode>



    <!-- CNCオプション -->
    <option>
        <hssb>
            <p>拡張ドライバ/ライブラリ機能が必要です。</p>
        </hssb>
    </option>



    <!-- CNCパラメータ -->
    <parameter>
        <type>
            <ul>
                <li>Series 15i<br/>
                    0021=16(EDITモード) (必ず設定して下さい)<br/>
                    0023=16(EDITモード以外) (必ず設定して下さい)<br/>
                </li>
            </ul>
        </type>
    </parameter>



    <!-- CNCモード -->
    <mode>
        <cmn>
            すべてのモードで使用できます。 (各登録データに応じたモードの設定を行う必要があります。)
        </cmn>
    </mode>



    <!-- 対応機種 -->
    <support>
        <!-- M（ミリング） -->
        <m>
            <fs0ia>H</fs0ia>
            <fs0ib>H</fs0ib>
            <fs0id>X</fs0id>
            <fs0if>X</fs0if>
            <fs15>H</fs15>
            <fs15i>H</fs15i>
            <fs16>H</fs16>
            <fs18>H</fs18>
            <fs21>H</fs21>
            <fs16ia>H</fs16ia>
            <fs18ia>H</fs18ia>
            <fs21ia>H</fs21ia>
            <fs16ib>H</fs16ib>
            <fs18ib>H</fs18ib>
            <fs21ib>H</fs21ib>
            <fs30ia>H</fs30ia>
            <fs30ib>H</fs30ib>
        </m>
        <!-- T（旋盤） -->
        <t>
            <fs0ia>H</fs0ia>
            <fs0ib>H</fs0ib>
            <fs0id>X</fs0id>
            <fs0if>X</fs0if>
            <fs15>H</fs15>
